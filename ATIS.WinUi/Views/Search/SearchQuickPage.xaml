    <Grid x:Name="ContentArea" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <!--<ProgressRing IsActive="True" />-->
        <TabView IsAddTabButtonVisible="False" SelectedIndex="{x:Bind ViewModel.SelectedReportTabIndex, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
            <TabView.TabItems>
                <TabViewItem
                    x:Uid="TabViewItem_Search"
                    IsClosable="False"
                    Visibility="Collapsed">
                    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="45" />
                            <RowDefinition Height="1*" />
                        </Grid.RowDefinitions>
                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                            <TextBlock
                                x:Uid="TextBlock_DataSetCount"
                                Margin="{StaticResource XSmallLeftTopRightBottomMargin}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Style="{ThemeResource DetailSubTitleStyle}" />
                            <TextBlock
                                Width="50"
                                Margin="{StaticResource XSmallLeftTopRightBottomMargin}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource BodyTextStyle}"
                                Text="{x:Bind ViewModel.DataSetCount, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                            <AutoSuggestBox
                                Name="SearchAutoSuggestBox"
                                Width="250"
                                Margin="{StaticResource XSmallLeftMargin}"
                                VerticalAlignment="Center"
                                CornerRadius="2"
                                Loaded="SearchAutoSuggestBox_OnLoaded"
                                PlaceholderText="Search"
                                QueryIcon="Find"
                                Text="{x:Bind ViewModel.FilterText, Mode=TwoWay}">
                                <interactivity:Interaction.Behaviors>
                                    <core:EventTriggerBehavior EventName="QuerySubmitted">
                                        <core:InvokeCommandAction Command="{x:Bind ViewModel.SearchRunCommand, Mode=OneWay}" InputConverter="{StaticResource ArgsConverter}" />
                                    </core:EventTriggerBehavior>
                                </interactivity:Interaction.Behaviors>
                            </AutoSuggestBox>
                            <AppBarButton
                                x:Uid="AppBarButtonPdf.Content"
                                Click="PdfPrintListAll"
                                ToolTipService.ToolTip="PDF">
                                <AppBarButton.Icon>
                                    <BitmapIcon ShowAsMonochrome="False" UriSource="ms-appx:///Assets/QuestPdfLogo.png" />
                                </AppBarButton.Icon>
                            </AppBarButton>
                            <CheckBox
                                Width="80"
                                Margin="{StaticResource SmallLeftMargin}"
                                Content="Auswahl 1"
                                IsChecked="True" />
                        </StackPanel>
